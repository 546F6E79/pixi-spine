{
  "scripts": {
    "compile:browserify": "cp ../bin/pixi-spine.js build/browserify/ && yarn browserify -- src/app.ts -p [ tsify --noImplicitAny ] -o build/browserify/pixi-spine.bundle.js",
    "compile:webpack": "yarn webpack -- --progress --profile --colors --display-error-details --display-cached",
    "compile:vanilla": "cp ../bin/pixi-spine.js build/vanillajs/ && tsc -p tsconfig.vanilla.json",
    "www": "yarn static-html-server -- -p 9995 -r build",
    "all": "yarn compile:browserify && yarn compile:webpack && yarn compile:vanilla && yarn www"
  },
  "dependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "browserify": "^14.0.0",
    "pixi-spine": "^1.3.8",
    "pixi.js": "^4.3.4",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.6",
    "static-html-server": "^0.1.2",
    "ts-helpers": "^1.1.2",
    "tsify": "^3.0.1",
    "typescript": "^2.1.5",
    "webpack": "~1.14"
  }
}
